{
  "name": "@spred/react",
  "description": "React integration for Spred",
  "homepage": "https://github.com/art-bazhin/spred-react",
  "repository": "git@github.com/art-bazhin/spred-react.git",
  "bugs": "https://github.com/art-bazhin/spred-react/issues",
  "version": "0.2.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@size-limit/preset-small-lib": "^11.0.2",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.48",
    "@types/react-test-renderer": "^18.0.7",
    "@types/use-sync-external-store": "^0.0.6",
    "babel-jest": "^29.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.4",
    "react-test-renderer": "^18.2.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.2",
    "standard-version": "^9.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@spred/core": ">=0.36.2",
    "react": ">=18.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "commit": "git-cz",
    "size": "size-limit",
    "release": "standard-version",
    "bench": "npx http-server"
  },
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "path": "dist/index.js"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
